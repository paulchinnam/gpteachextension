{
  "manifest_version": 3,
  "name": "GPTEACH by Paul, Ryan, & Manu",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/folder-sm.png",
      "32": "images/folder-med.png",
      "192": "images/folder-big.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
